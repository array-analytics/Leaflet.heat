{
  "name": "leaflet.heat",
  "version": "0.2.1",
  "description": "A tiny and fast Leaflet heatmap plugin that supports loading by common package loaders.",
  "homepage": "https://github.com/array-analytics/Leaflet.heat",
  "keywords": [
    "heatmap",
    "canvas",
    "visualization",
    "gis",
    "leaflet",
    "plugin"
  ],
  "author": "Vladimir Agafonkin",
  "contributors": [
    "Diego H. Bustamante"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/array-analytics/Leaflet.heat.git"
  },
  "main": "dist/leaflet-heat.js",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-mourner": "^2.0.3",
    "simpleheat": "~0.4.0",
    "uglify-js": "^3.3.9"
  },
  "eslintConfig": {
    "extends": "mourner",
    "globals": {
      "L": false,
      "simpleheat": false
    }
  },
  "scripts": {
    "test": "eslint src",
    "prepublish": "uglifyjs node_modules/simpleheat/simpleheat.js src/HeatLayer.js -c -m -o dist/leaflet-heat.js"
  },
  "license": "BSD-2-Clause",
  "jshintConfig": {
    "quotmark": "single",
    "globals": {
      "L": true,
      "simpleheat": true
    },
    "trailing": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "noempty": true,
    "nonbsp": true,
    "undef": true,
    "unused": true,
    "browser": true
  }
}
